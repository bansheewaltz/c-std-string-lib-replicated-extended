#include "tests_includes.h"

#suite memset
#test one_char_x3
    char str[10] = "";
    char exp[10] = "";
    s21_memset(str, 'a', 3);
    s21_memset(exp, 'a', 3);
    ck_assert_str_eq(str, exp);

#test one_char_xFull
    int s = 10;
    char str[10] = "";
    char exp[10] = "";
    s21_memset(str, 'a', s - 1);
    memset(exp, 'a', s - 1);
    ck_assert_str_eq(str, exp);



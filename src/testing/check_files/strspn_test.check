#include "tests_includes.h"

#suite strcspn
#test t1
    ck_assert_uint_eq(strspn("1234567890", "321"), s21_strspn("1234567890", "321"));

#test t2
    ck_assert_uint_eq(strspn("1234567890", "234"), s21_strspn("1234567890", "234"));

#test t3
    ck_assert_uint_eq(strspn("1234567890", ""), s21_strspn("1234567890", ""));

#test t4
    ck_assert_uint_eq(strspn("", "123"), s21_strspn("", "123"));

#test t5
    ck_assert_uint_eq(strspn("", ""), s21_strspn("", ""));

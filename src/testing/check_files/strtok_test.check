#include "s21_tests.h"

#suite strtok
#test t1
    ck_assert_pstr_eq(strtok("test1/test2/test3/test4", "/"), 
                      s21_strtok("test1/test2/test3/test4", "/"));

#test t2
    ck_assert_pstr_eq(strtok("test1*test2*test3/test4", "*"), 
                      s21_strtok("test1*test2*test3/test4", "*"));

#test t3 
    ck_assert_pstr_eq(strtok("test15test25test35test4", "5"), 
                      s21_strtok("test15test25test35test4", "5"));
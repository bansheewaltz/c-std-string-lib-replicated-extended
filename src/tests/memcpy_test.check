#include "tests_includes.h"

#test classic
    char *src = "123";
    char res[10] = "";
    char exp[10] = "";

    memcpy(exp, src, strlen(src));
    s21_memcpy(res, src, strlen(src));
    ck_assert_str_eq(res, exp);


#test dest_full_capacity
    char *src = "12345";
    char res[5] = "";
    char exp[5] = "";

    memcpy(exp, src, strlen(src));
    s21_memcpy(res, src, strlen(src));
    ck_assert_str_eq(res, exp);


#test empty_to_empy
    char *src = "";
    char res[5] = "";
    char exp[5] = "";

    memcpy(exp, src, strlen(src));
    s21_memcpy(res, src, strlen(src));
    ck_assert_str_eq(res, exp);


#test empty_to_empy_zero_size
    char *src = "";
    char res[5] = "";
    char exp[5] = "";

    memcpy(exp, src, 0);
    s21_memcpy(res, src, 0);
    ck_assert_str_eq(res, exp);


#test empty_to_full_not_terminated
    char *src = "";
    char res[5] = "12345";
    char exp[5] = "12345";

    memcpy(exp, src, strlen(src));
    s21_memcpy(res, src, strlen(src));
    ck_assert_str_eq(res, exp);


#test src_add_is_dest
    char src[5] = "12345";
    char *res = &src[0];
    char exp[5] = "12345";

    memcpy(src, src, strlen(src));
    s21_memcpy(res, res, strlen(src));
    ck_assert_str_eq(res, exp);


#test n_gt_src_len
    char *src = "123";
    char res[5] = "";
    char exp[5] = "";
    int n = 5;

    memcpy(exp, src, n);
    s21_memcpy(res, src, n);
    ck_assert_str_eq(res, exp);


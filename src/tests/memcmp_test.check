#include "tests_includes.h"

#suite memcmp
#test test1
   ck_assert_int_eq(memcmp("", "", 5), s21_memcmp("", "", 5));


#test test2
    ck_assert_int_eq(memcmp("3dd3d", "3dd3d", 5),
                   s21_memcmp("3dd3d", "3dd3d", 5));


#test test3
    ck_assert_int_eq(memcmp("3", "3", 2), s21_memcmp("3", "3", 2));


#test test4
   ck_assert_int_eq(memcmp("3dd3sdfgsdd", "dsfg3dd3d", 0),
                   s21_memcmp("3dd3sdfgsdd", "dsfg3dd3d", 0));

#test test5
    ck_assert_int_eq(memcmp("1234", "13", 2), s21_memcmp("1234", "13", 2));

#test test6
   ck_assert_int_eq(memcmp("", "dsfg3dd3d", 3),
                   s21_memcmp("", "dsfg3dd3d", 3));

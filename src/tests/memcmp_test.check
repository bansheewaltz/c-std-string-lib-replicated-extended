#include "tests_includes.h"

#suite memcmp
#test test1
    int r1, r2;
    char s1[10] = "";
    char s2[10] = "";

    r1 = memcmp(s1, s2, 5);
    r2 =s21_memcmp(s1, s2, 5);
    ck_assert_int_eq(s1, s2);


#test test2
    int r1, r2;
    char s1[5] = "12345ggg";
    char s2[5] = "12345ggg";

    r1 = memcmp(s1, s2, 5);
    r2 =s21_memcmp(s1, s2, 5);
    ck_assert_int_eq(s1, s2);


#test test3
    int r1, r2;
    char s1[10] = "f";
    char s2[10] = "f";

    r1 = memcmp(s1, s2, 2);
    r2 =s21_memcmp(s1, s2, 2);
    ck_assert_int_eq(s1, s2);


#test test4
    int r1, r2;
    char s1[10] = "fdsaijf";
    char s2[10] = "qwrrfgg";

    r1 = memcmp(s1, s2, 0);
    r2 =s21_memcmp(s1, s2, 0);
    ck_assert_int_eq(s1, s2);


#test test5
    int r1, r2;
    char s1[10] = "1234";
    char s2[10] = "13";

    r1 = memcmp(s1, s2, 2);
    r2 =s21_memcmp(s1, s2, 2);
    ck_assert_int_eq(s1, s2);